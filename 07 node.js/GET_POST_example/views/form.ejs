<html>
  <head>
    <title>form</title>
  </head>
  <body>
    <h1><%= title %></h1>
    <h2>GET 요청</h2>
    <form action="/getForm" method="get">
      <input type="text" name="id" placeholder="id">
      <input type="password" name="pw" placeholder="pw">
      <input type="submit" value="제출">
    </form>
    <h2>POST 요청</h2>
    <form method="post" action="/postForm">
      <input type="text" name="id" placeholder="id">
      <input type="password" name="pw" placeholder="pw">
      <input type="submit" value="제출">
    </form>
  </body>
</html>